{{#each bridgelists}}
  <div class="box box-link-hover-shadow">
    <h2 class="title">
      {{title}}
    </h2>
  <a href="/bridgelist/{{_id}}" class="button">
      <span class="icon is-small has-text-success">
        <i class="fas fa-folder-open"></i>
      </span>
    </a>
      <a href="/dashboard/deletebridgelist/{{_id}}" class="button">
      <i class="fas fa-trash"></i>
       </a>
  </div>
{{/each}}